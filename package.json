{
    "name": "vite-plugin-vue-vuefront",
    "version": "0.0.10",
    "description": "vite-plugin-vue-vuefront",
    "main": "dist/index.js",
    "types": "types/index.d.ts",
    "files": [
        "dist/**/*",
        "templates/**/*",
        "client.d.ts",
        "installComponents.js"
    ],
    "scripts": {
        "dev": "tsc -p . -w --incremental",
        "build": "rimraf dist && run-s build-bundle build-types",
        "build-bundle": "nodemon -e ts -x esbuild src/index.ts --bundle --platform=node --target=node12 --external:@vue/compiler-sfc --external:vuefront --external:lodash --external:dotenv --outfile=dist/index.js",
        "build-types": "tsc -p . --emitDeclarationOnly --outDir temp && api-extractor run && rimraf temp",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path . --lerna-package plugin-vue",
        "release": "node ../../scripts/release.js",
        "yalc-watch": "nodemon -e vue,js,scss,json -x yalc push"
    },
    "keywords": [
        "vite-plugin",
        "vuefront"
    ],
    "publishConfig": {
        "access": "public"
    },
    "license": "MIT",
    "devDependencies": {
      "@types/dotenv": "^8.2.0",
      "@types/node": "^16.7.13",
      "@types/stylelint": "^13.13.2",
      "@types/lodash": "^4.14.172",
      "@typescript-eslint/eslint-plugin": "^4.29.2",
      "@typescript-eslint/parser": "^4.29.2",
      "eslint": "^7.32.0",
      "eslint-import-resolver-typescript": "^2.4.0",
      "eslint-plugin-import": "^2.24.0",
      "nodemon": "^2.0.12",
      "typescript": "^4.4.2",
      "vite": "^2.5.5",
      "vuefront": "^0.4.6",
      "rimraf": "^3.0.2"
    },
    "dependencies": {
        "@vue/compiler-sfc": "^3.2.10",
        "apollo-boost": "^0.4.9",
        "cookie": "^0.4.1",
        "dotenv": "^10.0.0",
        "isomorphic-fetch": "^3.0.0",
        "lodash": "^4.17.21",
        "querystring": "^0.2.1",
        "vue-i18n": "9.1.7",
        "vue-meta": "^3.0.0-alpha.2",
        "vue3-lazyload": "^0.2.2-beta"
    }
}
